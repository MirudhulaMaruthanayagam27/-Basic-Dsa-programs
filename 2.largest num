//largest of num
class Main {
    public static void main(String[] args) {
        int[] arr={1,2,3};
        int n=arr.length;
        int largest=arr[0];
        for(int i=0;i<n;i++){
            if(arr[i]>largest){
                largest=arr[i];
            }
        }
        System.out.print(+largest);
    }
}
